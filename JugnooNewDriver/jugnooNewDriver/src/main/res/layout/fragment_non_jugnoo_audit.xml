<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">


    <RelativeLayout
        android:id="@+id/topBar"
        android:layout_width="720px"
        android:layout_height="96px"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@color/menu_black">

        <TextView
            android:id="@+id/textViewTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/Audit"
            android:textColor="@color/white"
            android:textSize="40px"/>

        <ImageView
            android:id="@+id/imageViewBack"
            android:layout_width="60.7px"
            android:layout_height="76.9px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10px"
            android:padding="20px"
            android:src="@drawable/back_white_selector"/>
    </RelativeLayout>


    <LinearLayout
        android:id="@+id/linearLayoutSelfAudit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="70px"
        android:orientation="vertical">
        
        <Button
            android:layout_width="617px"
            android:layout_height="99px"
            android:text="@string/self_audit"
            android:textColor="@color/white"
            android:background="@drawable/new_orange_btn_selector_rounded"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="27px"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="17px"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="23px"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayoutNJAutoBranding"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="70px"
        android:orientation="vertical">

        <Button
            android:layout_width="617px"
            android:layout_height="99px"
            android:text="@string/non_jugnoo_auto_branding"
            android:textColor="@color/white"
            android:background="@drawable/new_orange_btn_selector_rounded"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="27px"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="17px"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="23px"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayoutNJAutoAudit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="70px"
        android:orientation="vertical">

        <Button
            android:layout_width="617px"
            android:layout_height="99px"
            android:text="@string/non_jugnoo_auto_audit"
            android:textColor="@color/white"
            android:background="@drawable/new_orange_btn_selector_rounded"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="27px"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="17px"/>

        <TextView
            android:layout_width="617px"
            android:layout_height="wrap_content"
            android:text="@string/NA"
            android:gravity="center"
            android:layout_marginTop="23px"/>

    </LinearLayout>

</LinearLayout>