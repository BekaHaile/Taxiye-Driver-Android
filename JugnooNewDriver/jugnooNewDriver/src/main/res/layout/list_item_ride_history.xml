    <LinearLayout
        android:layout_width="fill_parent"
        android:id="@+id/rideHistoryItem"
        android:layout_height="240px"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="360px"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:id="@+id/RideDetails"
            android:layout_gravity="center_vertical"
            >

            <RelativeLayout
                android:id="@+id/dateTimeRl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:splitMotionEvents="false">

                <ImageView
                    android:id="@+id/imageViewTime"
                    android:layout_width="30px"
                    android:layout_height="30px"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15px"
                    android:src="@drawable/time"
                    android:tag="mwar"/>

                <TextView
                    android:id="@+id/dateTimeValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5px"
                    android:layout_toRightOf="@+id/imageViewTime"
                    android:singleLine="true"
                    android:text="23 June, 2014 18:88 AM"
                    android:textColor="@color/bg_grey_opaque"
                    android:textSize="28px"
                    />

            </RelativeLayout>

            <TextView
                android:id="@+id/textViewRideId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15px"
                android:layout_marginTop="10px"
                android:singleLine="true"
                android:text="Ride ID: 1111"
                android:textColor="@color/bg_grey_opaque"
                android:textSize="28px"/>


            <TextView
                android:id="@+id/textViewStatusString"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15px"
                android:layout_marginTop="10px"
                android:singleLine="true"
                android:text="Status"
                android:textColor="@color/bg_grey_opaque"
                android:textSize="30px"/>

            <LinearLayout
                android:id="@+id/distTimeRl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:splitMotionEvents="false">

                <ImageView
                    android:layout_width="30px"
                    android:layout_height="30px"
                    android:layout_marginLeft="15px"
                    android:src="@drawable/location"
                    android:tag="mwar"/>

                <TextView
                    android:id="@+id/distanceValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5px"
                    android:singleLine="true"
                    android:text="5.6 Km"
                    android:textColor="@color/bg_grey_opaque"
                    android:textSize="28px"/>

                <ImageView
                    android:layout_width="30px"
                    android:layout_height="30px"
                    android:layout_marginLeft="30px"
                    android:src="@drawable/time"
                    android:tag="mwar"/>

                <TextView
                    android:id="@+id/rideTimeValue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5px"
                    android:singleLine="true"
                    android:text="10 min"
                    android:textColor="@color/bg_grey_opaque"
                    android:textSize="28px"/>


                <!--<TextView-->
                    <!--android:id="@+id/waitTimeValue"-->
                    <!--android:layout_width="wrap_content"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:layout_marginLeft="50px"-->
                    <!--android:singleLine="true"-->
                    <!--android:text="Wait: 10 min"-->
                    <!--android:textColor="@color/bg_grey_opaque"-->
                    <!--android:textSize="30px"/>-->


            </LinearLayout>



        </LinearLayout>

        <LinearLayout
            android:layout_width="270px"
            android:layout_height="144px"
            android:orientation="vertical"
            android:layout_marginLeft="5px"
            android:layout_gravity="center_vertical"
            android:background="@drawable/background_white_rounded_black_border">

            <TextView
                android:id="@+id/textViewFare"
                android:layout_width="match_parent"
                android:layout_height="71px"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/rupee_zero"
                android:textColor="@color/blue_btn"
                android:textSize="55px"/>


            <ImageView
                android:layout_width="270px"
                android:layout_height="2px"
                android:background="@color/black"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <TextView
                    android:id="@+id/textViewCustomerPaid"
                    android:layout_width="132px"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0px"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="@string/rupee_zero"
                    android:textColor="@color/bg_grey_opaque"
                    android:textSize="32px"/>
                <TextView
                    android:id="@+id/textViewCustomerPaidtext"
                    android:layout_width="132px"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:singleLine="true"
                    android:layout_below="@+id/textViewCustomerPaid"
                    android:text="Paid in Cash"
                    android:textColor="@color/bg_grey_opaque"
                    android:textSize="20px"/>

                <ImageView
                    android:layout_width="2px"
                    android:layout_height="76px"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/imageViewVertical"
                    android:background="@color/black"/>

                <TextView
                    android:id="@+id/textViewBalance"
                    android:layout_width="132px"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:singleLine="true"
                    android:layout_toRightOf="@+id/imageViewVertical"
                    android:text="@string/rupee_zero"
                    android:textColor="@color/bg_grey_opaque"
                    android:textSize="32px"/>

                <TextView
                    android:id="@+id/textViewBalanceText"
                    android:layout_width="132px"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/textViewBalance"
                    android:layout_toRightOf="@+id/imageViewVertical"
                    android:gravity="center"
                    android:singleLine="true"
                    android:text="Account"
                    android:textColor="@color/bg_grey_opaque"
                    android:textSize="20px"/>



            </RelativeLayout>







        </LinearLayout>

        <LinearLayout
            android:layout_width="80px"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginLeft="10px">

            <ImageView
                android:id="@+id/imageViewRequestType"
                android:layout_width="45px"
                android:layout_height="45px"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20px"
                android:layout_marginLeft="10px"
                android:layout_marginTop="60px"
                android:src="@drawable/request_autos"
                android:tag="mwar"
                />

            <ImageView
                android:layout_width="40px"
                android:layout_height="38px"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="20px"
                android:layout_marginLeft="10px"
                android:layout_marginTop="55px"
                android:src="@drawable/arrow_selector"
                android:tag="mwar"
                />


        </LinearLayout>

    </LinearLayout>


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="10px"
        android:layout_marginTop="0px"
        android:layout_below="@+id/rideHistoryItem"
        android:background="@drawable/shadow_up"/>



</RelativeLayout>