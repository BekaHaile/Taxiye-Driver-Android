                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/share_activity_text_margin_right"
                android:text="0"
                android:textColor="@color/black"
                android:textSize="30px"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayoutJugnooCmsn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativeLayoutManualAdj"
            android:layout_marginLeft="-4px"
            android:layout_marginRight="-4px"
            android:layout_marginTop="20px">

            <TextView
                android:id="@+id/textViewJugnooCmsn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/share_activity_text_margin_left"
                android:text="@string/jugnoo_cmsn"
                android:textColor="@color/black"
                android:textSize="30px"/>

            <TextView
                android:id="@+id/textViewJugnooCmsnValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
