                android:layout_width="28px"
                android:layout_height="28px"
                android:layout_toRightOf="@+id/textViewJugnooCmsn"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="10px"
                android:layout_marginTop="2px"
                android:background="@drawable/subtract_icon"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayoutOutstandingAmnt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativeLayoutJugnooCmsn"
            android:layout_marginLeft="-4px"
            android:layout_marginRight="-4px"
            android:layout_marginTop="20px">

            <TextView
                android:id="@+id/textViewOutstandingAmnt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/share_activity_text_margin_left"
                android:text="@string/Outstanding_amnt"
                android:textColor="@color/black"
                android:textSize="30px"/>

            <TextView
                android:id="@+id/textViewOutstandingAmntValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/share_activity_text_margin_right"
                android:text="0"
                android:textColor="@color/black"
                android:textSize="30px"/>

            <ImageView
                android:id="@+id/imageViewNegetive1"
                android:layout_width="28px"
                android:layout_height="28px"
                android:layout_toRightOf="@+id/textViewOutstandingAmnt"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="10px"
                android:layout_marginTop="2px"
                android:background="@drawable/subtract_icon"/>

        </RelativeLayout>

        <ImageView
            android:id="@+id/imageViewCurrentInv1"
            android:layout_width="680px"
            android:layout_height="2px"
            android:layout_below="@+id/relativeLayoutOutstandingAmnt"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25px"
            android:background="@color/grey_dark_less"/>

        <RelativeLayout
            android:id="@+id/relativeLayoutTotalAmnt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageViewCurrentInv1"
            android:layout_marginLeft="-4px"
            android:layout_marginRight="-4px"
            android:layout_marginTop="25px">

            <TextView
                android:id="@+id/textViewTotalAmnt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/share_activity_text_margin_left"
                android:text="@string/acc_credited"
                android:textColor="@color/new_orange"
                android:textSize="36px"/>

            <TextView
                android:id="@+id/textViewTotalAmntValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/share_activity_text_margin_right"
                android:text="0"
                android:textColor="@color/new_orange"
                android:textSize="36px"/>

        </RelativeLayout>

        <ImageView
            android:id="@+id/imageViewCurrentInv4"
            android:layout_width="match_parent"
            android:layout_height="2px"
            android:layout_below="@+id/relativeLayoutTotalAmnt"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25px"
            android:background="@color/menu_black"/>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayoutEarnings"
        android:layout_width="match_parent"
        android:layout_height="144px"
        android:layout_marginLeft="-4px"
        android:layout_marginRight="-4px"
        android:layout_marginTop="-4px">

        <TextView
            android:id="@+id/textViewUpcomingInvoice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginTop="20px"
            android:layout_marginLeft="20px"
            android:text="@string/upcoming_payment_due_date"
            android:textColor="@color/black"
            android:textSize="32px"/>

        <TextView
            android:id="@+id/textViewUpcomingInvoiceDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="27 Jan 2106"
            android:layout_marginRight="20px"
            android:textColor="@color/black"
            android:textSize="32px"/>


    </RelativeLayout>



</LinearLayout>